PROOF "P→A, Q→(P∧B∧H), H→¬S ⊢ Q→(A∧¬S) "
INFER P→A,
     Q→(P∧B∧H),
     H→¬S 
     ⊢ Q→(A∧¬S)
FORMULAE
0 A∧¬S,
1 ¬S,
2 A,
3 H,
4 H→¬S,
5 P∧B∧H,
6 P∧B,
7 P,
8 P→A,
9 B,
10 Q,
11 Q→P∧B∧H,
12 Q→(P∧B∧H)
IS
SEQ ("→ intro"[A,B\10,0]) (cut[B,C\5,0]) ("→ elim"[A,B\10,5]) (hyp[A\11]) (hyp[A\10]) (cut[B,C\6,0]) (LAYOUT "∧ elim" (0) ("∧ elim(L)"[A,B\6,3]) (hyp[A\5])) (cut[B,C\7,0]) (LAYOUT "∧ elim" (0) ("∧ elim(L)"[A,B\7,9]) (hyp[A\6])) (cut[B,C\2,0]) ("→ elim"[A,B\7,2]) (hyp[A\8]) (hyp[A\7]) (cut[B,C\3,0]) (LAYOUT "∧ elim" (0) ("∧ elim(R)"[A,B\6,3]) (hyp[A\5])) (cut[B,C\1,0]) ("→ elim"[A,B\3,1]) (hyp[A\4]) (hyp[A\3]) (cut[B,C\0,0]) ("∧ intro"[A,B\2,1]) (hyp[A\2]) (hyp[A\1]) (hyp[A\0])
END
